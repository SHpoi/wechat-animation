doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width,initial-scale=1,user-scalable=0')
    title 漫迷微信公众号
    link(rel='stylesheet', href='./css/weui.min.css')
    link(rel='stylesheet', href='./css/swiper.min.css')
    link(rel='stylesheet', href='./css/example.css')
    link(rel='stylesheet', href='./css/mycss.css')
  body
    //.animation-history
      //p.animation-history-title
        //| 您上次观看的是 
        //span RE 从零开始的异世界生活
    .page__bd.page__bd_spacing.animation-welcome
      h1 欢迎来到漫迷微信公众号
    .animation-slider.swiper-container
      .swiper-wrapper
        .swiper-slide
          img(style='width:100%;height: 100%;', src='./img/0.jpg')
        .swiper-slide
          img(style='width:100%;height: 100%;', src='./img/1.jpg')
        .swiper-slide
          img(style='width:100%;height: 100%;', src='./img/2.jpg')
        .swiper-slide
          img(style='width:100%;height: 100%;', src='./img/3.jpg')
      // Add Pagination
      .swiper-pagination
    .page__bd.page__bd_spacing.animation-search
      // <a href="javascript:;" class="weui-btn weui-btn_primary">点击展现searchBar</a>
      #searchBar.weui-search-bar
        form.weui-search-bar__form
          .weui-search-bar__box
            i.weui-icon-search
            input#searchInput.weui-search-bar__input(type='search', placeholder='搜索', required='')
            a#searchClear.weui-icon-clear(href='javascript:')
          label#searchText.weui-search-bar__label
            i.weui-icon-search
            span 搜索
      #searchResult.weui-cells.searchbar-result(style='display: none')
    .page__bd.page__bd_spacing.animation-taglist
      .tag-grids
        a.tag-grid(href='javascript:;')
          .weui-grid__icon
            img(src='data:image/png;base64,', alt='')
          p.weui-grid__label 热血
        a.tag-grid(href='javascript:;')
          .weui-grid__icon
            img(src='data:image/png;base64,', alt='')
          p.weui-grid__label 竞技
        a.tag-grid(href='javascript:;')
          .weui-grid__icon
            img(src='data:image/png;base64,', alt='')
          p.weui-grid__label 百合
        a.tag-grid(href='javascript:;')
          .weui-grid__icon
            img(src='data:image/png;base64,', alt='')
          p.weui-grid__label 美食
        a.tag-grid(href='javascript:;')
          .weui-grid__icon
            img(src='data:image/png;base64,', alt='')
          p.weui-grid__label 校园
        a.tag-grid(href='javascript:;')
          .weui-grid__icon
            img(src='data:image/png;base64,', alt='')
          p.weui-grid__label 魔法
        a.tag-grid(href='javascript:;')
          .weui-grid__icon
            img(src='data:image/png;base64,', alt='')
          p.weui-grid__label 冒险
        a.tag-grid(href='javascript:;')
          .weui-grid__icon
            img(src='data:image/png;base64,', alt='')
          p.weui-grid__label 福利
    each i in animation
      .animation-suggest(style="display:none")
        .page__bd.page__bd_spacing.paddingright0.clearfix
          h4.animation-suggest-title
            | #{i.title}
          img.turn-left(src="data:image/png;base64,")
        .page__bd.page__bd_spacing.swiper-container.animation-suggest-container
          .swiper-wrapper
            -for(var j=0;j<i.slider.length;j++){
              .swiper-slide(animateId="#{i.slider[j]._id}")
                img.animation-poster.swiper-lazy(style='width:100%;height: 100%;', data-src='#{"./upload/"+i.slider[j].poster}')
                p.animation-titlebom.textOverhidden #{i.slider[j].title}
                .swiper-lazy-preloader.swiper-lazy-preloader-white
            -}
    script(type='text/javascript', src='./js/jquery.min.js')
    script(type='text/javascript', src='https://res.wx.qq.com/open/js/jweixin-1.0.0.js')
    script(src='https://res.wx.qq.com/open/libs/weuijs/1.0.0/weui.min.js')
    script(src='./js/swiper.min.js')
    script.
      $(function(){
        
        for(var t=0;t<#{animation.length};t++){
          (function(t,l){
            setTimeout(function(){
              $('.animation-suggest').eq(t).fadeIn(1000);
              var swiper = new Swiper($('.animation-suggest-container').eq(t), {
                nextButton: $('.turn-left').eq(t),
                slidesPerView: 3,
                paginationClickable: true,
                spaceBetween: 15,
                lazyLoading: true,
                preloadImages: false,
                loop:true
              });
              //最后一个
              if(t==(l-1)){
                var str='<div class="page__bd page__bd_spacing footerbottom"><div class="weui-footer">'+
                '<p class="weui-footer__links">'+
                '<a href="javascript:;" class="weui-footer__link">漫迷微信公众号首页</a>'+
                '</p>'+
                '<p class="weui-footer__text">Copyright © SHpoi</p>'+
                '</div></div>';
                $('body').append(str);
              }
            },(t+1)*1000)
          })(t,#{animation.length})
        }
      })
    script(src="./js/index.js")
      
