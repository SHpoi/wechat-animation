doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width,initial-scale=1,user-scalable=0')
    title Document
    link(rel='stylesheet', href='/css/weui.min.css')
    link(rel='stylesheet', href='/css/example.css')
    link(rel='stylesheet', href='/css/mycss.css')
  body
    .animation-detail
      .animation-detail-desc-pic
        img.previewPic(src='#{baseurl+"/upload/"+animation.poster}', alt='')
        .animation-detail-desc-messagelist
          .clearfix
            section.animation-detail-douban
              img.base64(src="data:image/png;base64,")
              p.douban-grade #{animation.doubanRating}
            //section.animation-detail-fenxiang
            //img.base64(src="data:image/png;base64,")
              //p.douban-grade 分享
          p.animation-country 国家：#{animation.country}
          p.animation-director 导演：#{animation.director}
          p.animation-year 年份：#{animation.year}
      .animation-top-title
        p.animation-title #{animation.title}
      .top-bg.blur
        img(src='#{baseurl+"/upload/"+animation.poster}', alt='')
    .page__bd.page__bd_spacing
      .animation-detail-but.clearfix
        a.weui-btn.weui-btn_plain-default.btn-want-watch(href='javascript:void(0)')
          img.lovelove.filllove( style="display:#{love?'inline-block':'none'}" src="data:image/png;base64,")
          img.lovelove.emptylove(style="display:#{!love?'inline-block':'none'}" src="data:image/png;base64,")
          |  喜欢
        a.weui-btn.weui-btn_plain-default.btn-comment(href='#comments') 评论
    article.weui-article.animation-summary
      section
        p
          if animation.summary 
            | #{String(animation.summary).slice(0,60)+'...'}
          else
            | #{'暂无简介'}
      i.icon.iconfont.icon-down.summary-down
    .page__bd.page__bd_spacing.animation-episode
      p 动漫集数
      div
        -if(animation.episode && animation.episode.length>0){
          -for(var i=0;i<animation.episode.length;i++){
            if i%4==0
              p.clearfix
                if i+4<animation.episode.length
                  a.animation-episode-item(href="javascript:void(0);") #{i+1}
                  a.animation-episode-item(href="javascript:void(0);") #{i+2}
                  a.animation-episode-item(href="javascript:void(0);") #{i+3}
                  a.animation-episode-item(href="javascript:void(0);") #{i+4}
                else
                  -for(var h=i;h<animation.episode.length;h++){
                  a.animation-episode-item(href="javascript:void(0);") #{h+1}
                  -}
          -}
        -}else{
        p.es-none 暂无动画
        -}
    // 评论模块
    .weui-panel.weui-panel_access
      .weui-panel__hd 动漫评论
      input#CommentNum(type='text', hidden='', value='#{comments.length}')
      .weui-panel__bd.comments-container
        if comments && comments.length > 0
          each item in comments
            .weui-media-box.weui-media-box_text
              h4.weui-media-box__title.comment-user-text #{item.from.name}
              p.weui-media-box__desc #{item.content}
      .weui-panel__ft
        a.js-commentReadMore.weui-cell.weui-cell_access.weui-cell_link(href='javascript:void(0);')
          .weui-cell__bd 查看更多
          span.weui-cell__ft
    #comments
      form#commentForm(action='', method='POST')
        input#movie(type='text', hidden='', name='comment[animation]', value='#{animation._id}')
        input#from(type='text', hidden='', name='comment[from]', value='#{user._id}')
        input#toId(type='text', hidden='', name='comment[tid]')
        input#commentId(type='text', hidden='', name='comment[cid]')
        .weui-cells__title 在评论或者回复谁时改动我
        .weui-cells.weui-cells_form
          .weui-cell
            .weui-cell__bd
              textarea#content.weui-textarea(placeholder='请输入文本', rows='3', name='comment[content]')
              .weui-textarea-counter
                span 0
                | /200
        .weui_btn_area.clearfix
          a#submit.weui-btn.weui-btn_mini.weui-btn_primary.comment-submit(href='javascript:;') 提交
    .page__bd.page__bd_spacing
      .weui-footer
        p.weui-footer__links
          a.weui-footer__link(href='javascript:;') 漫迷微信公众号首页
        p.weui-footer__text Copyright © SHpoi
    // mask
    div
      #iosMask.weui-mask(style='display: none')
      #iosActionsheet.weui-actionsheet
        article.weui-article
          section
            p
              if animation.summary && animation.summary.length > 300
                | #{String(animation.summary).slice(0, 300) + '...'}
              else
                | #{animation.summary}
    //loading
    #loadingToast(style='display:none;')
      .weui-mask_transparent
      .weui-toast
        i.weui-loading.weui-icon_toast
        p.weui-toast__content 数据加载中
    script(type='text/javascript', src='/js/jquery.min.js')
    script(type='text/javascript', src='https://res.wx.qq.com/open/js/jweixin-1.0.0.js')
    script(src='https://res.wx.qq.com/open/libs/weuijs/1.0.0/weui.min.js')
    script(type='text/javascript').
      wx.config({
          debug: false, // 开启调试模式,调用的所有api的返回值会在客户端alert出来，若要查看传入的参数，可以在pc端打开，参数信息会通过log打出，仅在pc端时才会打印。
          appId: 'wx46fcd895bde3d3a2', // 必填，公众号的唯一标识
          timestamp: '#{timestamp}', // 必填，生成签名的时间戳
          nonceStr: '#{noncestr}', // 必填，生成签名的随机串
          signature: '#{signature}',// 必填，签名，见附录1
          jsApiList: [
              'onMenuShareTimeline',
              'onMenuShareAppMessage',
              'onMenuShareQQ',
              'onMenuShareWeibo',
              'onMenuShareQZone',
              'previewImage'
          ] // 必填，需要使用的JS接口列表，所有JS接口列表见附录2
      })
      wx.ready(function(){
        console.log()
        //微信分享
        wx.checkJsApi({
            jsApiList: ['previewImage','onMenuShareTimeline','onMenuShareAppMessage','onMenuShareQQ','onMenuShareWeibo','onMenuShareQZone'],
            success: function(res) {
                console.log(res)
            }
        })
        var shareOpt={
            title: '#{animation.title}', // 分享标题
            desc: '更多精彩，尽在漫迷微信公众号', // 分享描述
            link: '#{baseurl+'/wechat/jump/'+animation._id}', // 分享链接
            imgUrl: '#{baseurl+"/upload/"+animation.poster}', // 分享图标
            success: function() {
                window.alert('分享成功')
            },
            cancel: function() {
                window.alert('分享失败')
            }
        }
        wx.onMenuShareAppMessage(shareOpt);
        wx.onMenuShareTimeline(shareOpt);
        wx.onMenuShareQQ(shareOpt);
        wx.onMenuShareWeibo(shareOpt);
        wx.onMenuShareQZone(shareOpt);

        $('.previewPic').unbind().click(function(){
          wx.previewImage({
              current: "#{baseurl+"/upload/"+animation.poster}",
              urls: ["#{baseurl+"/upload/"+animation.poster}"],
          })
        })

      })



      $(function() {
        //喜欢不喜欢
        $('.btn-want-watch').click(function(){
        var showchild=$(this).find('.lovelove:visible');
          if(showchild.hasClass('filllove')){
            //原本是喜欢，现在不喜欢
            ajaxLove(0,showchild)
          }else{
            //喜欢
            ajaxLove(1,showchild)
          }

        })
        function ajaxLove(islove,showchild){
          if ($loadingToast.css('display') != 'none') return;

          $loadingToast.fadeIn(100);

          $.post('#{baseurl+"/wechat/animationlove"}?love='+islove+'&ani_id=#{animation._id}&user_id=#{user._id}',function(data){
            if(data.success==1){
              $loadingToast.fadeOut(100);
              if(!islove){
                alert('人家这么好看，你居然不喜欢了')
              }else if(islove){
                alert('关注该动漫成功')
              }
              showchild.hide();
              showchild.siblings('.lovelove').css({'display':'inline-block'});
            }
          })
        }
        //简介栏操作
        var $iosActionsheet = $('#iosActionsheet');
        var $iosMask = $('#iosMask');
        function hideActionSheet() {
          $iosActionsheet.removeClass('weui-actionsheet_toggle');
          $iosMask.fadeOut(200);
        }
        $iosMask.on('click', hideActionSheet);
        $('.summary-down').on("click", function () {
          $iosActionsheet.addClass('weui-actionsheet_toggle');
          $iosMask.fadeIn(200);
        });

        //评论提交
        $('#submit').click(function(e) {
          e.preventDefault();
          $.ajax({
            type: 'POST',
            data: {
              comment: {
                animation: $('#movie').val(),
                from: $('#from').val(),
                tid: $('#toId').val(),
                cid: $('#commentId').val(),
                content: $('#content').val()
              }
            },
            url: '/user/comment',
            success: function(results) {
              if (results.success === 1) {
                window.location.reload();
              }
            }
          })
        })

        $('.js-commentReadMore').click(function(e){
          //alert($('#movie').val()+' '+$('#CommentNum').val())

        })
        //loading提示
        var $loadingToast = $('#loadingToast');
        $('.js-commentReadMore').on('click', function () {
          if ($loadingToast.css('display') != 'none') return;

          $loadingToast.fadeIn(100);

          $.ajax({
            type: 'POST',
            data: {
              comment: {
                animation: $('#movie').val(),
                start: $('#CommentNum').val()
              }
            },
            url: '/user/searchComment',
            success: function (results) {
              if (results.success === 1) {
                $loadingToast.fadeOut(100);

                $('#CommentNum').val(Number($('#CommentNum').val()) + results.Searchcomments.length);
                var str = '';
                for (var i in results.Searchcomments) {
                  str += '<div class="weui-media-box weui-media-box_text">' +
                          '<h4 class="weui-media-box__title comment-user-text">' + results.Searchcomments[i].from.name + '</h4>' +
                          '<p class="weui-media-box__desc">' + results.Searchcomments[i].content + '</p>' +
                          '</div>'
                }
                $('.comments-container').append(str);
              }
            }
          })
        });
      })